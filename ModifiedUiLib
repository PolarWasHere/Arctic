-- Scripts:

local function MQKCSA_fake_script() -- DropDown.LocalScript 
	local script = Instance.new('LocalScript', DropDown)

	local button = false
	script.Parent.MouseButton1Down:Connect(function()
		local Sound = Instance.new("Sound", game:GetService("Workspace"))
		Sound.SoundId = "rbxassetid://421058925"
		Sound:Play()
		spawn(function()
			wait(1.3)
			Sound:Destroy()
		end)
		if button == true then
			button = false
	
			local RotationTween = {}
			RotationTween.Rotation = -90
			local Info = TweenInfo.new(0.2, Enum.EasingStyle.Sine)
			game:GetService("TweenService"):Create(script.Parent.Layers, Info, RotationTween):Play()
			
			local MenuTween = {}
			MenuTween.Size = UDim2.new(0, 336, 0, 0)
			game:GetService("TweenService"):Create(script.Parent.DropDown, Info, MenuTween):Play()
		else
			button = true
	
			local RotationTween = {}
			RotationTween.Rotation = 90
			local Info = TweenInfo.new(0.2, Enum.EasingStyle.Sine)
			game:GetService("TweenService"):Create(script.Parent.Layers, Info, RotationTween):Play()
			
			local MenuTween = {}
			MenuTween.Size = UDim2.new(0, 336, 0, 90)
			game:GetService("TweenService"):Create(script.Parent.DropDown, Info, MenuTween):Play()
		end
	end)
end
coroutine.wrap(MQKCSA_fake_script)()
end

