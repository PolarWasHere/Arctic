Value.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Value.BackgroundTransparency = 1.000
Value.BorderColor3 = Color3.fromRGB(27, 42, 53)
Value.BorderSizePixel = 0
Value.Position = UDim2.new(0.550595224, 0, 0.586956501, 0)
Value.Size = UDim2.new(0, 141, 0, 9)
Value.Font = Enum.Font.GothamSemibold
Value.Text = "0"
Value.TextColor3 = Color3.fromRGB(255, 255, 255)
Value.TextXAlignment = Enum.TextXAlignment.Right

-- Scripts:

local function AOIMH_fake_script() -- Slider.LocalScript 
	local script = Instance.new('LocalScript', Slider)

	script.Parent.MouseButton1Down:Connect(function()
		local Sound = Instance.new("Sound", game:GetService("Workspace"))
		Sound.SoundId = "rbxassetid://421058925"
		Sound:Play()
		spawn(function()
			wait(1.3)
			Sound:Destroy()
		end)
	end)
end
coroutine.wrap(AOIMH_fake_script)()
local function CRULU_fake_script() -- Slider.LocalScript 
	local script = Instance.new('LocalScript', Slider)

	
end
coroutine.wrap(CRULU_fake_script)()
local function MHQWI_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	local UserInputService = game:GetService("UserInputService")
	local Dragging = false
	script.Parent.TextButton.MouseButton1Down:Connect(function()
		Dragging = true
	end)
	
	UserInputService.InputChanged:Connect(function()
		if Dragging then
			local MousePos = UserInputService:GetMouseLocation()+Vector2.new(0,36)
			local RelPos = MousePos-script.Parent.AbsolutePosition
			local Precent = math.clamp(RelPos.X/script.Parent.AbsoluteSize.X,0,1)
			script.Parent.TextButton.Position = UDim2.new(Precent,0,0.5,0)
			script.Parent.Parent.Value.Text = tostring(math.floor(Precent * tonumber(Max)))
		end
	end)
	
	UserInputService.InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			Dragging = false
		end
	end)
end
coroutine.wrap(MHQWI_fake_script)()

Slider.MouseButton1Down:Connect(function()
    pcall(callback, tonumber(Value.Text))
end)

end

end
