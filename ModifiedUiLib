local function CUDK_fake_script() -- ColorPicker.LocalScript 
	local script = Instance.new('LocalScript', ColorPicker)

	script.Parent.MouseButton1Down:Connect(function()
		local Sound = Instance.new("Sound", game:GetService("Workspace"))
		Sound.SoundId = "rbxassetid://421058925"
		Sound:Play()
		spawn(function()
			wait(1.3)
			Sound:Destroy()
		end)
	end)
