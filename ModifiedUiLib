coroutine.wrap(CUDK_fake_script)()
local function ZXYMR_fake_script() -- ColorPicker.LocalScript 
	local script = Instance.new('LocalScript', ColorPicker)

	local Red = script.Parent.red
	local Green = script.Parent.green
	local Blue = script.Parent.blue
	
	RedValue = 6
	GreenValue = 6
	BlueValue = 6
	
	Red:GetPropertyChangedSignal'Text':Connect(function()
		if tonumber(Red.Text) ~= nil and tonumber(Red.Text) > 255 then
			Red.Text = 255
		elseif tonumber(Red.Text) ~= nil and tonumber(Red.Text) < 0 then
			Red.Text = 0
		end
		
		if tonumber(Red.Text) == nil then
			Red.Text = ""
		end
		
		RedValue = tonumber(Red.Text)
	end)
	
	Green:GetPropertyChangedSignal'Text':Connect(function()
		if tonumber(Green.Text) ~= nil and tonumber(Green.Text) > 255 then
			Green.Text = 255
		elseif tonumber(Green.Text) ~= nil and tonumber(Green.Text) < 0 then
			Green.Text = 0
		end
	
		if tonumber(Green.Text) == nil then
			Green.Text = ""
		end
	
		GreenValue = tonumber(Green.Text)
	end)
	
	Blue:GetPropertyChangedSignal'Text':Connect(function()
		if tonumber(Blue.Text) ~= nil and tonumber(Blue.Text) > 255 then
			Blue.Text = 255
		elseif tonumber(Blue.Text) ~= nil and tonumber(Blue.Text) < 0 then
			Blue.Text = 0
		end
	
		if tonumber(Blue.Text) == nil then
			Blue.Text = ""
		end
	
		BlueValue = tonumber(Blue.Text)
	end)
	
	while wait() do
		pcall(function()
			script.Parent.Color.BackgroundColor3 = Color3.fromRGB(RedValue, GreenValue, BlueValue)
		end)	
	end
end
coroutine.wrap(ZXYMR_fake_script)()
local function DCNDQZ_fake_script() -- ColorPicker.LocalScript 
	local script = Instance.new('LocalScript', ColorPicker)

	script.Parent.MouseButton1Down:Connect(function()
		ColorChoice = Color3.fromRGB(RedValue, GreenValue, BlueValue)
		CreateNotification("Color Set")
	end)
end
coroutine.wrap(DCNDQZ_fake_script)()
 
end

